

<div class="artist-container">
{{#each artist}} 

  <div class="artist-info">
   
    <img src="{{this.images.[1].url}}" alt="artist-image">
    <div class="album-link">
      <h3>{{this.name}}</h3>
      <button><a href="/albums/{{this.id}}">View Albums</a></button>
    </div>
  
  </div>

{{/each}}

</div>